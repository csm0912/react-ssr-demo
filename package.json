    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^4.1.0",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "hot-module-replacement": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
